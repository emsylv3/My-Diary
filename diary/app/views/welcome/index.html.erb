<h1>Welcome#index</h1>
<p>Find me in app/views/welcome/index.html.erb</p>
		<div>
			<h1>My favorite websites</h1>
			<% for url, name in @websites %>
				<li><a href="<%= url %>"><%= name %></a></li>
				<% end %>
		</div>
		<img src="http://railsgirls.com/images/rg-warsaw.png" /> 
		<h1>My Diary</h1>
		<% for entry in @entries %>
			<h2><%=entry.title %></h2>
			<p><%=entry.date %></p>
			<p><%=entry.contents %></p>
			<% end %>
		
		
